{
  "name": "datannur",
  "version": "0.11.0",
  "description": "datannur is a portable data catalog that can run without a server",
  "type": "module",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "open-cli app/index.html",
    "serve" : "vite preview",
    "test": "vitest",
    "release": "node make_release.js",
    "static-make-js": "vite build && node app/static_make/js/make.js",
    "static-make-py": "vite build && bash app/static_make/py/setup.sh && bash app/static_make/py/make.sh"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "jsonjsdb_editor": "^0.2.10",
    "open-cli": "^8.0.0",
    "puppeteer": "^24.12.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "serve-handler": "^6.1.6",
    "sitemap": "^8.0.0",
    "svelte": "^5.35.6",
    "svelte-preprocess": "^6.0.3",
    "vite": "^7.0.4",
    "vite-plugin-full-reload": "^1.2.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "datatables.net": "^2.3.2",
    "datatables.net-bm": "^2.3.2",
    "datatables.net-buttons": "^3.2.4",
    "datatables.net-buttons-bm": "^3.2.4",
    "datatables.net-fixedcolumns": "^5.0.4",
    "datatables.net-fixedcolumns-bm": "^5.0.4",
    "datatables.net-fixedheader": "^4.0.3",
    "datatables.net-fixedheader-bm": "^4.0.3",
    "datatables.net-scroller": "^2.4.3",
    "datatables.net-scroller-bm": "^2.4.3",
    "diff": "^8.0.2",
    "file-saver": "^2.0.5",
    "fitty": "^2.4.2",
    "flexsearch": "^0.8.205",
    "jquery": "^3.7.1",
    "jquery-powertip": "^1.3.2",
    "jsonjsdb": "^0.3.14",
    "jszip": "^3.10.1",
    "marked": "^16.0.0",
    "mermaid": "^11.8.1",
    "minimasonry": "^1.3.2",
    "navigo": "^8.11.1"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "editor.formatOnSave": true,
    "semi": false,
    "arrowParens": "avoid"
  }
}
