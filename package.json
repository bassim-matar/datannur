{
  "name": "datannur",
  "version": "0.15.0",
  "description": "datannur is a portable data catalog that can run without a server",
  "license": "MIT",
  "type": "module",
  "private": true,
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "dev:proxy": "python3 public/python-scripts/proxy-llm.py",
    "build": "vite build",
    "build:analyze": "BUNDLE_VIEW=true vite build",
    "build:extension": "cd vscode-extension && npm install && npm run package",
    "preview": "open-cli app/index.html",
    "serve": "vite preview",
    "test:unit": "vitest run --exclude=test/ui.test.ts",
    "test:ui": "npm run build && vitest run test/ui.test.ts",
    "test:extension": "cd vscode-extension && npm run test",
    "test": "npm run test:unit && npm run test:ui && npm run test:extension",
    "test:no-build": "vitest run",
    "node:ts": "node --experimental-strip-types --disable-warning=ExperimentalWarning",
    "static-make": "vite build && npm run node:ts app/node-scripts/static-make.ts",
    "deploy": "npm run node:ts app/node-scripts/deploy.ts",
    "static-deploy": "npm run static-make && npm run deploy",
    "lint": "eslint --ext .ts,.svelte && cd vscode-extension && npm run lint",
    "type-check": "tsc --noEmit && cd vscode-extension && npm run type-check",
    "svelte-check": "svelte-check --workspace src",
    "format": "prettier --write . '!**/*.json.js'",
    "format:check": "prettier --check . '!**/*.json.js'",
    "validate-schemas": "npm run node:ts public/node-scripts/validate-schemas.ts",
    "schema:generate": "npm run node:ts -- public/node-scripts/manage-schemas.ts generate",
    "schema:generate:strict": "npm run node:ts -- public/node-scripts/manage-schemas.ts generate --strict",
    "schema:update": "npm run node:ts -- public/node-scripts/manage-schemas.ts update",
    "schema:update:strict": "npm run node:ts -- public/node-scripts/manage-schemas.ts update --strict",
    "schema:reset": "npm run node:ts -- public/node-scripts/manage-schemas.ts reset",
    "api:generate": "npm run node:ts public/node-scripts/generate-api.ts",
    "api:generate:dev": "NODE_ENV=development npm run node:ts public/node-scripts/generate-api.ts",
    "api:dev": "npm run node:ts public/api/nodejs/server.ts",
    "schema:build": "npm run validate-schemas && npm run api:generate",
    "check": "npm run format:check && npm run lint && npm run type-check && npm run svelte-check && npm run validate-schemas",
    "verify": "npm run check && npm run test"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-alias": "^6.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/escape-html": "^1.0.4",
    "@types/file-saver": "^2.0.7",
    "@types/jquery": "^3.5.33",
    "@types/minimasonry": "^1.3.5",
    "@types/serve-handler": "^6.1.4",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.0",
    "jiti": "^2.6.1",
    "jsonjsdb-builder": "^0.6.5",
    "open-cli": "^8.0.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.94.2",
    "serve-handler": "^6.1.6",
    "svelte": "^5.45.2",
    "svelte-check": "^4.3.4",
    "svelte-eslint-parser": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vite-plugin-full-reload": "^1.2.0",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "datatables.net": "^2.3.5",
    "datatables.net-bm": "^2.3.5",
    "datatables.net-buttons": "^3.2.5",
    "datatables.net-buttons-bm": "^3.2.5",
    "datatables.net-fixedcolumns": "^5.0.5",
    "datatables.net-fixedcolumns-bm": "^5.0.5",
    "datatables.net-fixedheader": "^4.0.5",
    "datatables.net-fixedheader-bm": "^4.0.5",
    "datatables.net-scroller": "^2.4.3",
    "datatables.net-scroller-bm": "^2.4.3",
    "diff": "^8.0.2",
    "dompurify": "^3.3.0",
    "escape-html": "^1.0.3",
    "file-saver": "^2.0.5",
    "fitty": "^2.4.2",
    "flexsearch": "^0.8.212",
    "jquery": "^3.7.1",
    "jquery-powertip": "^1.3.2",
    "jsonjsdb": "^0.8.5",
    "jszip": "^3.10.1",
    "marked": "^17.0.1",
    "mermaid": "^11.12.1",
    "minimasonry": "^1.3.2",
    "svelte-fileapp": "^0.0.11"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid",
    "singleQuote": true
  }
}
