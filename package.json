{
  "name": "datannur",
  "version": "0.12.4",
  "description": "datannur is a portable data catalog that can run without a server",
  "license": "MIT",
  "type": "module",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "open-cli app/index.html",
    "serve": "vite preview",
    "test": "vitest",
    "release": "node --experimental-strip-types make_release.ts",
    "static-make": "vite build && node --experimental-strip-types --disable-warning=ExperimentalWarning app/static_make/make.ts"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "autoprefixer": "^10.4.21",
    "jsonjsdb_editor": "^0.3.2",
    "open-cli": "^8.0.0",
    "puppeteer": "^24.19.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.92.1",
    "serve-handler": "^6.1.6",
    "sitemap": "^8.0.0",
    "svelte": "^5.38.8",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-full-reload": "^1.2.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "datatables.net": "^2.3.4",
    "datatables.net-bm": "^2.3.4",
    "datatables.net-buttons": "^3.2.5",
    "datatables.net-buttons-bm": "^3.2.5",
    "datatables.net-fixedcolumns": "^5.0.5",
    "datatables.net-fixedcolumns-bm": "^5.0.5",
    "datatables.net-fixedheader": "^4.0.3",
    "datatables.net-fixedheader-bm": "^4.0.3",
    "datatables.net-scroller": "^2.4.3",
    "datatables.net-scroller-bm": "^2.4.3",
    "diff": "^8.0.2",
    "file-saver": "^2.0.5",
    "fitty": "^2.4.2",
    "flexsearch": "^0.8.212",
    "jquery": "^3.7.1",
    "jquery-powertip": "^1.3.2",
    "jsonjsdb": "^0.4.1",
    "jszip": "^3.10.1",
    "marked": "^16.2.1",
    "mermaid": "^11.11.0",
    "minimasonry": "^1.3.2",
    "navigo": "^8.11.1"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "editor.formatOnSave": true,
    "semi": false,
    "arrowParens": "avoid"
  }
}
